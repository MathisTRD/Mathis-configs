{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "white": "#ffffff",
    "bgcolor": "#777777"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "background",
          "foreground": "white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true,
            "style": "full"
          },
          "style": "plain",
          "template": "  {{ .Path }}  ",
          "type": "path"
        },

        {
          "background": "white",
          "foreground": "black",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "background",
          "foreground": "white",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "  ->",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}